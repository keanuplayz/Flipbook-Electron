{
  "name": "flipbook-flectron",
  "productName": "Flipbook Electron",
  "copyright": "Copyright Â© 2020 Keanu",
  "description": "The CCDirectLink tool Flipbook packaged into an Electron application.",
  "keywords": [],
  "main": "./main.js",
  "version": "1.0.0",
  "author": "Keanu",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.keanuplayz.flipbook-electron",
    "files": [
      "**/*",
      "build/icon.*",
      "./view.js",
      "./media",
      "./data"
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    }
  },
  "dependencies": {},
  "devDependencies": {
    "electron": "5.0.0-beta.7",
    "electron-builder": "^21.2.0",
    "electron-builder-squirrel-windows": "^22.2.0"
  }
}
